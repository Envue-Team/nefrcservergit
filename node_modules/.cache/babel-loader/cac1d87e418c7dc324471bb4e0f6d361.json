adge-danger mr-2\"\n      @click=\"deleteVolunteer\"\n    >\n      Delete\n    </button>\n\n    <button type=\"submit\" class=\"badge badge-success\"\n      @click=\"updateVolunteer\"\n    >\n      Update\n    </button>\n    <p>{{ message }}</p>\n  </div>\n\n  <div v-else>\n    <br />\n    <p>Please click on a Volunteer...</p>\n  </div>\n</template>\n\n<script>\nimport DataService from \"../services/DataService\";\n\nexport default {\n  name: \"volunteer\",\n  data() {\n    return {\n      currentVolunteer: null,\n      message: ''\n    };\n  },\n  methods: {\n    getVolunteer(id) {\n      DataService.get(id)\n        .then(response => {\n          this.currentVolunteer = response.data;\n          console.log(response.data);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n\n    updatePublished(status) {\n      var data = {\n        id: this.currentVolunteer.id,\n        title: this.currentVolunteer.name,\n        description: this.currentVolunteer.description,\n        published: status\n      };\n\n      DataService.update(this.currentVolunteer.id, data)\n        .then(response => {\n          this.currentVolunteer.published = status;\n          console.log(response.data);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n\n    updateVolunteer() {\n      DataService.update(this.currentVolunteer.id, this.currentVolunteer)\n        .then(response => {\n          console.log(response.data);\n          this.message = 'The volunteer was updated successfully!';\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n\n    deleteVolunteer() {\n      DataService.delete(this.currentVolunteer.id)\n        .then(response => {\n          console.log(response.data);\n          this.$router.push({ name: \"volunteers\" });\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    }\n  },\n  mounted() {\n    this.message = '';\n    this.getVolunteer(this.$route.params.id);\n  }\n};\n</script>\n\n<style>\n.edit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":"src/views"}]}