QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAPA;AAQA;AA1CA,GAVA;AAsDA,EAAA,OAtDA,qBAsDA;AACA,SAAA,kBAAA;AACA;AAxDA,CAAA","sourcesContent":["<template>\n  <div class=\"list row\">\n    <div class=\"col-md-8\">\n      <div class=\"input-group mb-3\">\n        <input type=\"text\" class=\"form-control\" placeholder=\"Search by name\"\n          v-model=\"name\"/>\n        <div class=\"input-group-append\">\n          <button class=\"btn btn-outline-secondary\" type=\"button\"\n            @click=\"searchTitle\"\n          >\n            Search\n          </button>\n        </div>\n      </div>\n    </div>\n    <div class=\"col-md-6\">\n      <h4>Volunteers List</h4>\n      <ul class=\"list-group\">\n        <li class=\"list-group-item\"\n          :class=\"{ active: index == currentIndex }\"\n          v-for=\"(volunteer, index) in volunteers\"\n          :key=\"index\"\n          @click=\"setActiveVolunteer(volunteer, index)\"\n        >\n          {{ volunteer.name }}\n        </li>\n      </ul>\n\n      <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllVolunteers\">\n        Remove All\n      </button>\n    </div>\n    <div class=\"col-md-6\">\n      <div v-if=\"currentVolunteer\">\n        <h4>Volunteer</h4>\n        <div>\n          <label><strong>Title:</strong></label> {{ currentVolunteer.name }}\n        </div>\n        <div>\n          <label><strong>Description:</strong></label> {{ currentVolunteer.description }}\n        </div>\n        <div>\n          <label><strong>Status:</strong></label> {{ currentVolunteer.published ? \"Published\" : \"Pending\" }}\n        </div>\n\n        <a class=\"badge badge-warning\"\n          :href=\"'/Volunteers/' + currentVolunteer.id\"\n        >\n          Edit\n        </a>\n      </div>\n      <div v-else>\n        <br />\n        <p>Please click on a Volunteer...</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport DataService from \"../services/DataService\";\n\nexport default {\n  name: \"volunteers-list\",\n  data() {\n    return {\n      volunteers: [],\n      currentVolunteer: null,\n      currentIndex: -1,\n      name: \"\"\n    };\n  },\n  methods: {\n    retrieveVolunteers() {\n      DataService.getAll()\n        .then(response => {\n          this.volunteers = response.data;\n        })\n        .catch(e => {\n          console.log(e.message);\n        });\n    },\n\n    refreshList() {\n      this.retrieveVolunteers();\n      this.currentVolunteers = null;\n      this.currentIndex = -1;\n    },\n\n    setActiveVolunteer(volunteer, index) {\n      this.currentVolunteer = volunteer;\n      this.currentIndex = index;\n    },\n\n    removeAllVolunteers() {\n      DataService.deleteAll()\n        .then(response => {\n          console.log(response.data);\n          this.refreshList();\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n    \n    searchTitle() {\n      DataService.findByName(this.name)\n        .then(response => {\n          this.volunteers = response.data;\n          console.log(response.data);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    }\n  },\n  mounted() {\n    this.retrieveVolunteers();\n  }\n};\n</script>\n\n<style>\n.list {\n  text-align: left;\n  max-width: 750px;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":"src/views"}]}